{
  "name": "supla/api-client",
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Swagger\\Client\\": "lib/",
      "Supla\\ApiClient\\": "src/"
    }
  },
  "scripts": {
    "phpcs": "phpcs src --standard=phpcs-rules.xml",
    "phpcbf": "phpcbf src --standard=phpcs-rules.xml"
  }
}
